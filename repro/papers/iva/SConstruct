\newcommand{\dR}{R^{\dagger}}
\newcommand{\bZ}{{\bf Z}}
\newcommand{\bR}{{\bf R}}
\newcommand{\bx}{{\bf x}}
\newcommand{\by}{{\bf y}}
\newcommand{\bh}{{\bf h}}
\newcommand{\bp}{{\bf p}}
\newcommand{\op}{\bar{p}}
